{
  "manifest_version": 2,
  "name": "InfoGata Extension",
  "version": "1.0.0",
  "description": "Extension for Audiogata.com, Videogata.com, and ReaderGata.com",
  "icons": {
    "16": "../icons/16.png",
    "19": "../icons/19.png",
    "38": "../icons/38.png",
    "48": "../icons/48.png",
    "128": "../icons/128.png"
  },
  "background": {
    "scripts": ["../src/index.ts"]
  },
  "browser_action": {
    "default_title": "InfoGata Extension",
    "default_popup": "../src/popup.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "extension@infogata.com"
    }
  },
  "permissions": ["<all_urls>", "storage", "tabs"],
  "web_accessible_resources": ["../src/hook.ts", "../src/content.ts"]
}
