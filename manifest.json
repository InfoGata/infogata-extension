{
  "manifest_version": 2,
  "name": "InfoGata Extension",
  "version": "1.0.0",
  "description": "Extension for Audiogata, Videogata, and ReaderGata",
  "icons": {
    "64": "icons/64.png",
    "128": "icons/128.png",
    "256": "icons/256.png"
  },
  "background": {
    "scripts": ["src/index.ts"]
  },
  "browser_action": {
    "default_title": "InfoGata Extension",
    "default_popup": "src/popup.html"
  },
  "permissions": ["<all_urls>", "storage", "tabs"],
  "web_accessible_resources": ["src/hook.ts", "src/content.ts"]
}
