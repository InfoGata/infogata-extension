{
  "manifest_version": 2,
  "name": "MediaGata Extension",
  "version": "1.0.0",
  "description": "Extension for AudioGata and VideoGata",
  "icons": {
    "64": "icons/64.png",
    "128": "icons/128.png",
    "256": "icons/256.png"
  },
  "background": {
    "scripts": [ "src/index.ts" ]
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "tabs"
  ],
  "web_accessible_resources": [
    "src/hook.ts",
    "src/content.ts"
  ]
}
